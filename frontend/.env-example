# =======================================================
# VARIABLES DE ENTORNO - FRONTEND EXAMPLE
# Este archivo muestra todas las variables necesarias
# Copia a .env-dev o .env-prod y ajusta los valores
# =======================================================

# === API CONFIGURATION ===
REACT_APP_BACKEND_URL=http://localhost:8000
REACT_APP_API_URL=http://localhost:8000/api
REACT_APP_FRONTEND_URL=http://localhost:3000

# === PAYMENT PROCESSING ===
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here

# === ENVIRONMENT ===
REACT_APP_ENV=development
REACT_APP_DEBUG=true

# === FEATURES FLAGS ===
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_ERROR_TRACKING=false
REACT_APP_ENABLE_LOGGING=true

# === BUILD CONFIGURATION ===
GENERATE_SOURCEMAP=true
INLINE_RUNTIME_CHUNK=false

# === DEV SERVER ===
PORT=3000
BROWSER=none
FAST_REFRESH=true

# === FILE UPLOAD ===
REACT_APP_MAX_FILE_SIZE=5242880
REACT_APP_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif

# === MAPS INTEGRATION ===
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# === NOTIFICATIONS ===
REACT_APP_ENABLE_PUSH_NOTIFICATIONS=false

# === URLS DE REDIRECCIÃ“N ===
REACT_APP_SUCCESS_URL=http://localhost:3000/reservation-confirmation/exito
REACT_APP_CANCEL_URL=http://localhost:3000/reservation-confirmation/error